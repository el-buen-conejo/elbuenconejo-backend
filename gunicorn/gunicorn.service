[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/elbuenconejo-backend
ExecStart=/home/ubuntu/env/bin/gunicorn \
    --access-logfile - \
    --workers 3 \
    --bind 0.0.0.0:8000 \
    rabbits_farm.wsgi:application

       
[Install]
WantedBy=multi-user.target